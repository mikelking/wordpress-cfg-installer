{
	"name"             : "mikelking/wordpress-config-installer",
	"description"      : "A custom installer to handle deploying WordPress config with composer",
	"keywords"         : ["wordpress"],
	"type"             : "composer-plugin",
	"license"          : "GPL-2.0+",
	"minimum-stability": "dev",
	"authors"          : [
		{
			"name" : "Mikel King",
			"email": "mikel.king@olivent.com"
		}
	],
	"autoload"         : {
		"psr-0": {
			"mikelking\\Composer\\": "src/"
		}
	},
	"extra"            : {
		"class": "mikelking\\Composer\\WordPressConfigPlugin"
	},
	"require"          : {
		"composer-plugin-api": "^1.0"
	},
	"require-dev"      : {
		"composer/composer": "1.0.*@dev"
	},
	"conflict"         : {
		"composer/installers": "<1.0.6"
	}
}
